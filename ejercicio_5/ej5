S_0x557f83b1f160 .scope module, "err_mod" "check_err" 2 11, 3 1 0, S_0x557f83b1e5c0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in1"
    .port_info 1 /INPUT 4 "in2"
    .port_info 2 /OUTPUT 1 "out1"
    .port_info 3 /OUTPUT 1 "out2"
v0x557f83b1f2e0_0 .var "err1", 0 0;
v0x557f83b3f1a0_0 .var "err2", 0 0;
v0x557f83b3f260_0 .net "in1", 3 0, v0x557f83b3f840_0;  1 drivers
v0x557f83b3f350_0 .net "in2", 3 0, v0x557f83b3f910_0;  1 drivers
v0x557f83b3f430_0 .var "out1", 0 0;
v0x557f83b3f540_0 .var "out2", 0 0;
    .scope S_0x557f83b1f160;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557f83b1f2e0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x557f83b1f160;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557f83b3f1a0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x557f83b1f160;
T_2 ;
    %delay 1, 0;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x557f83b3f260_0;
    %pad/u 32;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %load/vec4 v0x557f83b3f260_0;
    %pad/u 32;
    %cmpi/u 9, 0, 32;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x557f83b1f2e0_0;
T_2.0 ;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x557f83b3f350_0;
    %pad/u 32;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %load/vec4 v0x557f83b3f350_0;
    %pad/u 32;
    %cmpi/u 9, 0, 32;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x557f83b3f1a0_0;
T_2.2 ;
    %load/vec4 v0x557f83b1f2e0_0;
    %cassign/vec4 v0x557f83b3f430_0;
    %cassign/link v0x557f83b3f430_0, v0x557f83b1f2e0_0;
    %load/vec4 v0x557f83b3f1a0_0;
    %cassign/vec4 v0x557f83b3f540_0;
    %cassign/link v0x557f83b3f540_0, v0x557f83b3f1a0_0;
    %end;
    .thread T_2;
    .scope S_0x557f83b1e5c0;
T_3 ;
    %vpi_func 2 13 "$value$plusargs" 32, "in1=%b", v0x557f83b3f840_0 {0 0 0};
    %pad/u 1;
    %store/vec4 v0x557f83b3f780_0, 0, 1;
    %vpi_func 2 14 "$value$plusargs" 32, "in2=%b", v0x557f83b3f910_0 {0 0 0};
    %pad/u 1;
    %store/vec4 v0x557f83b3f780_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x557f83b3f9e0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x557f83b3fad0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x557f83b3f840_0;
    %pad/u 8;
    %load/vec4 v0x557f83b3f910_0;
    %pad/u 8;
    %add;
    %addi 6, 0, 8;
    %store/vec4 v0x557f83b3f680_0, 0, 8;
    %vpi_call 2 20 "$display", "First number: %b", v0x557f83b3f840_0 {0 0 0};
